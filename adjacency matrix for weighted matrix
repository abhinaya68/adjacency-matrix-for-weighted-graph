graph=[(1,2,5),(2,3,9),(3,4,4),(4,5,8),(1,3,4),(2,4,2),(3,5,2)]
vertexset=set()
adj_matrix=[]
for edge in graph:
    vertexset.add(edge[0])
    vertexset.add(edge[1])
nv=len(vertexset)
print(nv)
adj_matrix=[[0]*nv for i in range(nv)]
for row in adj_matrix:
    print(row)
for edge in graph:
    adj_matrix[edge[0]-1][edge[1]-1]=edge[2]
    adj_matrix[edge[1]-1][edge[0]-1]=edge[2]
print("adj_matrix")
for row in adj_matrix:
    print(row)
